{
  "name": "tallyinsight-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "package": "npm run electron:build"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "express": "^5.1.0",
    "lucide-react": "^0.554.0",
    "next": "15.1.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "recharts": "^3.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.3",
    "electron-builder": "^26.0.12",
    "typescript": "~5.8.2",
    "wait-on": "^9.0.3"
  }
}